@model CarModelYear


<h1>Editmodelyear</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action= "SaveEditmodelyear" asp-controller="ModelYear" asp-route-id="@Model.ID">
         
           
            <div class="form-group">
                <label asp-for="ModelYear" class="control-label"></label>
                <input asp-for="ModelYear" class="form-control" />
                <span asp-validation-for="ModelYear" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Edit" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a href="/ModelYear/GetAll">All Model Year</a>
</div>

