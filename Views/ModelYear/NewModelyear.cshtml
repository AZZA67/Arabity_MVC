@model CarModelYear


<h1>NewModelyear</h1>


<div class="row">
    <div class="col-md-4">
        <form asp-action="SaveNewModelyear">
          
       
            <div class="form-group">
                <label asp-for="ModelYear" class="control-label"></label>
                <input asp-for="ModelYear" class="form-control" />
                <span asp-validation-for="ModelYear" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a href="/ModelYear/GetAll">All Model Year</a>
</div>