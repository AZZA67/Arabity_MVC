@model CarModelName



<h1>Edit Model Name</h1>


<div class="row">
    <div class="col-md-4">
        <form asp-action="SaveEditModelname"  asp-controller="CarModelName" asp-route-id="@Model.ID" asp-route-brandid="@Model.BrandId">
          
           
            <div class="form-group">
                <label asp-for="ModelName" class="control-label"></label>
                <input asp-for="ModelName" class="form-control" />
                <span asp-validation-for="ModelName" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


<div>
    <a href="/CarModelName/GetAllinBrandid">All Brand</a>
</div>

