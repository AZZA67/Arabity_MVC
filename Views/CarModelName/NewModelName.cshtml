@model CarModelName





<h4>New CarModelName</h4>

<div class="row">
    <div class="col-md-4">
        <form asp-action="SaveNewModelNameinbrand" asp-controller="CarModelName" asp-route-id="@ViewBag.brandid">
    
         
            <div class="form-group">
                <label asp-for="ModelName" class="control-label ml-5 mt-5"></label>
                <input asp-for="ModelName" class="form-control ml-5" />
                <span asp-validation-for="ModelName" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary ml-5" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="ml-5" href="/CarModelName/GetAllinBrandid/@ViewBag.brandid">All ModelsName</a>
</div>


