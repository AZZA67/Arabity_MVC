@model CarBrand

<h1>EditBrand</h1>


<div class="row">
    <div class="col-md-4">
        <form asp-action="SaveEditBrand" asp-controller="Brand" asp-route-id="@Model.ID">
   
            
            <div class="form-group">
                <label asp-for="BrandName" class="control-label ml-5"></label>
                <input asp-for="BrandName" class="form-control ml-5" />
                <span asp-validation-for="BrandName" class="text-danger ml-5"></span>
            </div>
            <div class="controls">
              @*<input type="file" asp-for="Files" class="floatLabel ml-5">*@
         </div>
            <div class="form-group">
                <input type="submit" value="Edit" class="btn btn-primary ml-5" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="ml-5" href="/Home/Index">All Brand</a>
</div>




